{
  "name": "NestJS-CRUD-with-ReactJS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "frontend": "cd frontend && yarn dev",
    "backend": "cd backend && yarn dev",
    "dev": "concurrently \"yarn frontend\" \"yarn backend\"",
